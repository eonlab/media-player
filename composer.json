{
    "name": "open-admin-ext/media-player",
    "description": "Intergrates mediaelement into open-admin",
    "type": "library",
    "keywords": ["open-admin", "extension", "audio", "video", "player"],
    "homepage": "https://github.com/open-admin-ext/media-player",
    "license": "MIT",
    "authors": [
        {
            "name": "z-song",
            "email": "zosong@126.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "open-admin-org/open-admin": "~1.0",
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "OpenAdmin\\Admin\\MediaPlayer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenAdmin\\Admin\\MediaPlayer\\MediaPlayerServiceProvider"
            ]
        }
    }
}
